{
  "plugins": [ "mocha" ],
  "rules": {
    "no-use-before-define": 1,
    "strict": 0,
    "comma-dangle": 1,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "prefer-template": 0,
    "new-cap": 0,
    "global-require": 0,
    "consistent-return": 0,
    "max-len": [2, 120]
  },
  "env": {
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}
